<!-- tr = table row; td = table data; thead: table header -->
<table>
  <thead>
    <tr>
      <th colspan="1"> Attribute </th>
      <th colspan="1"> Value </th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td> Name </td>
      <td><%= cat.name %></td>
    </tr>
    
    <tr>
      <td> Age </td>
      <td><%= cat.age %></td>
    </tr>
    
    <tr>
      <td> Gender </td>
      <td><%= cat.sex %></td>
    </tr>
    
    <tr>
      <td> Birthday </td>
      <td><%= cat.birth_date %></td>
    </tr>
    
    <tr>
      <td> Color </td>
      <td><%= cat.color %></td>
    </tr>
    
    <tr>
      <td> Description </td>
      <td><%= cat.description %></td>
    </tr>
  </tbody>
</table>

